{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist" // или любая другая директория для сборки TypeScript
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}
